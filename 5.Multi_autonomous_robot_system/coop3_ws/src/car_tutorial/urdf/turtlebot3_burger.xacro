<?xml version="1.0" ?>
<robot name="turtlebot3_burger" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <!-- Include common properties -->
    <!-- <xacro:include filename="$(find turtlebot3_description)/urdf/common_properties.urdf"/> -->

    <!-- Define material colors -->
    <xacro:property name="black" value="0.0 0.0 0.0 1.0"/>
    <xacro:property name="dark" value="0.3 0.3 0.3 1.0"/>
    <xacro:property name="light_black" value="0.4 0.4 0.4 1.0"/>
    <xacro:property name="blue" value="0.0 0.0 0.8 1.0"/>
    <xacro:property name="green" value="0.0 0.8 0.0 1.0"/>
    <xacro:property name="grey" value="0.5 0.5 0.5 1.0"/>
    <xacro:property name="orange" value="1.0 0.4235 0.0392 1.0"/>
    <xacro:property name="brown" value="0.8706 0.8118 0.7647 1.0"/>
    <xacro:property name="red" value="0.8 0.0 0.0 1.0"/>
    <xacro:property name="white" value="1.0 1.0 1.0 1.0"/>

    <!-- Base Link -->
    <link name="base_footprint"/>

    <!-- Base Joint -->
    <joint name="base_joint" type="fixed">
            <parent link="base_footprint"/>
            <child link="base_link"/>
            <origin xyz="0.0 0.0 0.010" rpy="0 0 0"/>
    </joint>

    <!-- Base Link Definition -->
    <link name="base_link">
        <visual>
            <origin xyz="-0.032 0 0.0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://turtlebot3_gazebo/models/turtlebot3_common/meshes/burger_base.dae" scale="0.0015 0.0015 0.0015"/>
            </geometry>
            <material name="light_black"/>
        </visual>

        <collision>
            <origin xyz="-0.048 0 0.105" rpy="0 0 0"/>
            <geometry>
                <box size="0.210 0.210 0.2145"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.239e+00"/>
            <inertia ixx="4.977e-03" ixy="-1.844e-05" ixz="5.240e-05" iyy="4.768e-03" iyz="-7.518e-06" izz="4.510e-03"/>
        </inertial>
    </link>

    <!-- Left Wheel Joint -->
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0.0 0.12 0.0345" rpy="-1.57 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <!-- Left Wheel Definition -->
    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <geometry>
                <mesh filename="package://turtlebot3_gazebo/models/turtlebot3_common/meshes/tire.dae" scale="0.0015 0.0015 0.0015"/>
            </geometry>
            <material name="dark"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.027" radius="0.0495"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="2.8498940e-02"/>
            <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09" iyy="1.1192413e-05" iyz="-1.4400107e-11" izz="2.0712558e-05"/>
        </inertial>
    </link>

    <!-- Right Wheel Joint -->
    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0.0 -0.120 0.0345" rpy="-1.57 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <!-- Right Wheel Definition -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <geometry>
                <mesh filename="package://turtlebot3_gazebo/models/turtlebot3_common/meshes/tire.dae" scale="0.0015 0.0015 0.0015"/>
            </geometry>
            <material name="dark"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.027" radius="0.0495"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="2.8498940e-02"/>
            <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09" iyy="1.1192413e-05" iyz="-1.4400107e-11" izz="2.0712558e-05"/>
        </inertial>
    </link>

    <!-- Caster Back Joint -->
    <joint name="caster_back_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_back_link"/>
        <origin xyz="-0.1215 0 -0.006" rpy="-1.57 0 0"/>
    </joint>

    <!-- Caster Back Link Definition -->
    <link name="caster_back_link">
        <collision>
            <origin xyz="0 0.001 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.045 0.0135 0.030"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="0.0075"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <!-- IMU Joint -->  
    <joint name="imu_joint" type="fixed">
        <parent link="base_link"/>
        <child link="imu_link"/>
        <origin xyz="-0.048 0 0.0102" rpy="0 0 0"/>
    </joint>

    <!-- IMU Link -->
    <link name="imu_link"/>

    <!-- Scan Joint -->
    <joint name="scan_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_scan"/>
        <origin xyz="-0.032 0 0.172" rpy="0 0 0"/>
    </joint>

    <!-- Scan Link Definition -->
    <link name="base_scan">
        <visual>
            <origin xyz="0 0 0.0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://turtlebot3_gazebo/models/turtlebot3_common/meshes/lds.dae" scale="0.0015 0.0015 0.0015"/>
            </geometry>
            <material name="dark"/>
        </visual>

        <collision>
            <origin xyz="0.015 0 -0.0065" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.0315" radius="0.055"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.129"/>
            <inertia ixx="1.57e-05" ixy="0.0" ixz="0.0" iyy="1.57e-05" iyz="0.0" izz="1.57e-05"/>
        </inertial>
    </link>

</robot>
